import java.util.*;
import java.io.*;
public class Main
{
	public static void main(String[] args) {
		ArrayList<Integer> l1=new ArrayList<Integer>();
		l1.add(12);
		l1.add(10);
		l1.add(43);
		l1.add(90);
		l1.add(21);
		l1.add(90);
		System.out.println("The Arraylist before sorting" +l1);
		Collections.sort(l1);
		System.out.println("The arraylist after sorting"+l1);
	}
}
